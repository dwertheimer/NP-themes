{
	"name": "Toothbleach JGC",
	"style": "Light",
	"author": {
		"name": "Jonathan Clark"
	},
	"editor": {
		"backgroundColor": "#FAFAFA",
		"altBackgroundColor": "#F9F9F9",
		"toolbarBackgroundColor": "#EBEBE4",
		"tintColor": "#C87230",
		"tintColor2": "#769EB3",
		"textColor": "#222E33",
		"toolbarIconColor": "#222E33",
		"menuItemColor": "#222E33",
		"timeBlockColor": "#E9C062",
		"shouldOverwriteFont": true
	},
	
	"__orderedStyles": [
		"title-mark1",
		"title-mark2",
		"title-mark3",
		"body",
		"quote-content",
		"bold",
		"bold-left-mark",
		"bold-right-mark",
		"italic",
		"italic-left-mark",
		"italic-right-mark",
		"boldItalic",
		"boldItalic-left-mark",
		"boldItalic-right-mark",
		"code",
		"code-left-backtick",
		"code-right-backtick",
		"special-char",
		"checked-todo-characters",
		"todo",
		"checked",
		"quote-mark",
		"tabbed",
		"NoteLinks-main",
		"link",
		"hashtag",
		"attag",
		"schedule-to-date-link",
		"done-date",
		"schedule-from-date-link",
		"title1",
		"title2",
		"title3",
		"note-title-link"
	],

	"styles": {
		
		"body": {
			"color": "#222E33",
			"font": "AvenirNext",
			"lineSpacing": 1,
			"paragraphSpacing": 6,
			"paragraphSpacingBefore": 0
		},
		
		"title1": {
			"color": "#C5487A",
			"size": 28,
			"font": "AvenirNext-Bold"
		},
		"title-in-frontmatter": {
			"regex": "^[tT]itle:\\s+(.*)",
			"matchPosition": 1,
			"color": "#C5487A",
			"size": 24,
			"font": "AvenirNext-Bold"
		},
		"title2": {
			"color": "#8B82C9",
			"size": 24,
			"font": "AvenirNext-Bold"
		},
		"title3": {
			"color": "#8B82C9",
			"size": 20,
			"font": "AvenirNext-Bold"
		},
		"title4": {
			"color": "#8B82C9",
			"size": 18,
		   "font": "AvenirNext-Bold"
		},

		"title-mark1": {
			"color": "#66DAE3E8",
			"size": 28,
			"paragraphSpacing": 3,
			"font": "AvenirNext-DemiBold"
		},
		"title-mark2": {
		   "color": "#66E9C062",
		   "size": 24,
			"paragraphSpacingBefore": 16,
			"paragraphSpacing": 3,
		   "font": "AvenirNext-DemiBold"
		},
		"title-mark3": {
			"color": "#66E9C062",
			"font": "AvenirNext-DemiBold",
			"size": 20,
			"paragraphSpacingBefore": 12,
			"paragraphSpacing": 3
		},
		"title-mark4": {
			"color": "#66E9C062",
			"font": "AvenirNext-DemiBold",
		   "paragraphSpacingBefore": 12,
			"paragraphSpacing": 3
		},

		"bold": {
			"color": "#996666",
			"font": "AvenirNext-Bold"
		},
		"bold-left-mark": {
			"color": "#CC6666"
		},
		"bold-right-mark": {
			"color": "#CC6666"
		},
		"italic": {
			"color": "#774444",
			"font": "AvenirNext-Italic"
		},
		"italic-left-mark": {
			"color": "#BB8888"
		},
		"italic-right-mark": {
			"color": "#BB8888"
		},
		"boldItalic": {
			"color": " #996666",
			"font": "AvenirNext-BoldItalic"
		},
		"boldItalic-left-mark": {
			"color": "#CC6666"
		},
		"boldItalic-right-mark": {
			"color": "#CC6666"
		},
		
		"todo": {
			"font": "noteplanstate",
			"color": "#CC6666",
			"headIndent": 33
		},
		
		"checked-todo-characters": {
			"font": "noteplanstate",
			"headIndent": 33
		},
		
		"special-char": {
			"font": "Menlo-Regular"
		},
		
		"tabbed": {
			"headIndent": 43
		},
		
		"quote-mark": {
			"color": "#979893",
			"font": "noteplanstate",
			"headIndent": 33
		},
		
		"quote-content": {
			"color": "#434343",
			"type": "italic",
			"headIndent": 33
		},
		
		"markdown-link": {
			"color": "#C89240"
		},
		
		"markdown-link-url": {
			"color": "#465E68"
		},
		
		"link": {
			"color": "#C88E6B"
		},
		
		"time": {
			"color": "#E36868"
		},
		
		"schedule-to-date-link": {
			"color": "#E8AE9B"
		},
		
		"done-date": {
			"color": "#BBBBBB"
		},
		
		"schedule-from-date-link": {
			"color": "##E8AE9B"
		},
		
		"note-title-link": {
			"color": "#97B853"
		},
		
		"hashtag": {
			"color": "#88482B",
			"type": ""
		},
		
		"attag": {
			"color": "#88482B",
			"type": ""
		},
		
		"phonenumber": {
			"color": "#769EB3"
		},

		"attribute": {
			"regex": "^([^:]{3,})(::)",
			"color": "#FF769EB3",
			"matchPosition": 1
		},
		"attribute-right-colons": {
			"regex": "^([^:]{3,})(::)",
			"color": "#99769EB3",
			"matchPosition": 2,
			"isMarkdownCharacter": true
		},

		"highlighted": {
			"regex": "(==)([^\\s].+?)(==)",
			"backgroundColor": "#55D2D21B",
			"order": 35,
			"matchPosition": 2,
			"isRevealOnCursorRange": true
		},
		"highlighted-left-marker": {
			"regex": "(==)([^\\s].+?)(==)",
			"color": "#AA45A2E5",
			"backgroundColor": "#7745A2E5",
			"isMarkdownCharacter": true,
			"isHiddenWithoutCursor": true,
			"isRevealOnCursorRange": true,
			"matchPosition": 1
		},
		"highlighted-right-marker": {
			"regex": "(==)([^\\s].+?)(==)",
			"color": "#AA45A2E5",
			"backgroundColor": "#7745A2E5",
			"isMarkdownCharacter": true,
			"isHiddenWithoutCursor": true,
			"isRevealOnCursorRange": true,
			"matchPosition": 3
		},

		"NoteLinks-main": {
			"regex": "\\h(\\[\\[)(.*?)(\\]\\])",
			"matchPosition": 2,
			"color": "#C5487A"
		},
		"NoteLinks-main-done": {
			"regex": "\\h(x\\[\\[)(.*?)(\\]\\])",
			"matchPosition": 1,
			"isMarkdownCharacter": true,
			"isRevealOnCursorRange": true,
			"font": "noteplanstate",
			"color": "#97B853"
		},
		"NoteLinks-main-cancelled": {
			"regex": "\\h(-\\[\\[)(.*?)(\\]\\])",
			"matchPosition": 1,
			"isMarkdownCharacter": true,
			"isRevealOnCursorRange": true,
			"font": "noteplanstate",
			"color": "#C5487A"
		},

		"NoteLinks-left-brackets": {
			"regex": "\\h(\\[\\[)(.*?)(\\]\\])",
			"matchPosition": 1,
			"color": " #C5487A",
			"isMarkdownCharacter": true,
			"isHiddenWithoutCursor": true,
			"isRevealOnCursorRange": true
		},
		"NoteLinks-right-brackets": {
			"regex": "\\h(\\[\\[)(.*?)(\\]\\])",
			"matchPosition": 3,
			"color": " #C5487A",
			"isMarkdownCharacter": true,
			"isHiddenWithoutCursor": true,
			"isRevealOnCursorRange": true
		},

		"strikethrough": {
			"regex": "(~~)([^~]+)(~~)",
			"matchPosition": 2,
			"strikethroughStyle": 2,
			"isRevealOnCursorRange": true
		},
		"strikethrough-left-tilde": {
			"regex": "(~~)([^~]+)(~~)",
			"matchPosition": 1,
			"isMarkdownCharacter": true,
			"isHiddenWithoutCursor": true,
			"color": "#d9d9d9",
			"isRevealOnCursorRange": true
		},
		"strikethrough-right-tilde": {
			"regex": "(~~)([^~]+)(~~)",
			"matchPosition": 3,
			"isMarkdownCharacter": true,
			"color": "#d9d9d9",
			"isHiddenWithoutCursor": true,
			"isRevealOnCursorRange": true
		},

		"checked": {
			"regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\] )(.*)",
			"color": "#DD97B863",
			"matchPosition": 0,
			"headIndent": 33
		},
		"checked-canceled": {
			"regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] )(.*)",
			"matchPosition": 0,
			"color": "#44C74853",
			"headIndent": 33,
			"strikethroughStyle": 1
		},
		"checked-scheduled": {
			"regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] )(.*)",
			"matchPosition": 0,
			"color": "#447B7C86",
			"headIndent": 33
		},

		"deadline": {
			"regex": "\\h+!(\\d{4}-\\d{2}-\\d{2})\\h*",
			"matchPosition": 1,
			"color": "#AA45A2E5"
		},

		"flagged-1": {
			"regex": "(^|\\v)\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)(\\!{1}\\h+.*)($|\\v)",
			"matchPosition": 4,
			"backgroundColor": "#FFE5E5"
		},

		"starred": {
			"regex": "(^|\\v)(.*⭐️.*)($|\\v)",
			"matchPosition": 2,
			"backgroundColor": "#FFE5E5"
		},

		"code": {
			"font": "CascadiaCode-Regular",
			"color": "#3B3AB2",
			"backgroundColor": "#DEEDFB"
		},
		"code-fence": {
			"theme": "github",
			"body": {
				"font": "Menlo-Regular",
				"backgroundColor": "#d5e4e2"
			},
			"corner-radius": 8,
			"fence-open": {
				"color": "#6584A2"
			},
			"fence-close": {
				"color": "#6584A2"
			},
			"language-specifier": {
				"font": "Menlo-Italic",
				"color": "#afafaf"
			}
		},
		"#waiting": {
			"regex": "^* (.*#waiting.*)$",
			"matchPosition": 1,
			"textColor": "#222E3388",
			"font": "AvenirNext-Italic"
		},
		"Templates_tags-opener": {
			"regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
			"matchPosition": 1,
			"color": "#6584A2",
			"font": "Menlo-Regular"
		},
		"Templates_tags": {
			"regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
			"matchPosition": 2,
			"color": "#3B3AB2",
			"font": "Menlo-Regular"
		},
		"Templates_tags-closer": {
			"regex": "(<%[\\-=_~]?\\s+)(.*?)(\\s+[\\-]?%>)",
			"matchPosition": 3,
			"color": "#6584A2",
			"font": "Menlo-Regular"
		},
		"Templates_tags-commented-out": {
			"regex": "(<%#\\s+.*?\\s+[\\-]?%>)",
			"matchPosition": 1,
			"color": "#6584A2",
			"font": "AvenirNext-Italic"
		},
		"eventID": {
		  "regex": "(event:[A-F0-9-]{36,37})",
		  "matchPosition": 1,
		  "color": "#CCA949",
		  "isHiddenWithoutCursor": true,
		  "isRevealOnCursorRange": true
		},
		"timeblocks": {
			"regex": "(?:^\\s*(?:\\*(?!\\s+\\[[\\-\\>]\\])\\s+|\\-(?!\\h+\\[[\\-\\>]\\]\\s+)|[\\d+]\\.|\\#{1,5}\\s+))(?:\\[\\s\\]\\s+)?.*?\\s(((at|from)\\s+([0-2]?\\d|noon|midnight)(:[0-5]\\d)?(\\s?(AM|am|PM|pm)?)(\\s*(\\-|\\–|\\~|\\〜|to)\\s*([0-2]?\\d)(:[0-5]\\d)?(\\s*(AM|am|PM|pm)?))?|([0-2]?\\d|noon|midnight)(:[0-5]\\d)\\s*(AM|am|PM|pm)?(\\s*(\\-|\\–|\\~|\\〜|to)\\s*([0-2]?\\d|noon|midnight)(:[0-5]\\d)?(\\s*(AM|am|PM|pm)?)?)?))(?=\\s|$)",
		  "matchPosition": 1,
		  "color": "#CC4999"
		}
	}
}
