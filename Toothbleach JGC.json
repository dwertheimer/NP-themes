{
    "name": "Toothbleach JGC",
    "style": "Light",
    "author": {
        "name": "Jonathan Clark"
    },

    "editor": {
        "backgroundColor": "#FAFAFA",
        "altBackgroundColor": "#F9F9F9",
        "tintColor": "#D8A250",
        "tintColor2": "#769EB3",
        "textColor": "#222E33",
        "toolbarBackgroundColor": "#EBEBE4",
        "toolbarIconColor": "#222E33",
        "menuItemColor": "#222E33",
        "shouldOverwriteFont": false
    },
    
    "orderedStyles": [
        "title-mark1",
        "title-mark2",
        "title-mark3",
        "body",
        "quote-content",
        "bold",
        "bold-left-mark",
        "bold-right-mark",
        "italic",
        "italic-left-mark",
        "italic-right-mark",
        "boldItalic",
        "boldItalic-left-mark",
        "boldItalic-right-mark",
        "code",
        "code-left-backtick",
        "code-right-backtick",
        "special-char",
        "checked-todo-characters",
        "todo",
        "checked",
        "quote-mark",
        "tabbed",
        "NoteLinks-main",
        "link",
        "hashtag",
        "attag",
        "schedule-to-date-link",
        "done-date",
        "schedule-from-date-link",
        "title1",
        "title2",
        "title3",
        "note-title-link"
    ],

    "styles": {
        
        "body": {
            "color": "#222E33",
            "font": ".AppleSystemUIFont",
            "lineSpacing": 1,
            "paragraphSpacing": 6,
            "paragraphSpacingBefore": 0
        },
        
        "title1": {
            "color": "#C5487A",
            "size": 28,
            "font": "AvenirNext-Bold"
        },
        "title2": {
            "color": "#8B82C9",
            "size": 24,
            "font": "AvenirNext-Bold"
        },
        "title3": {
            "color": "#8B82C9",
            "size": 20,
            "font": "AvenirNext-Bold"
        },
        "title4": {
            "color": "#8B82C9",
            "size": 18,
           "font": "AvenirNext-Bold"
        },

        "title-mark1": {
            "color": "#9B82C9",
            "size": 28,
            "paragraphSpacing": 3,
            "font": "AvenirNext-DemiBold"
        },
        "title-mark2": {
           "color": "#9B82C9",
           "size": 24,
            "paragraphSpacingBefore": 16,
            "paragraphSpacing": 3,
           "font": "AvenirNext-DemiBold"
        },
        "title-mark3": {
            "color": "#9B82C9",
            "font": "AvenirNext-DemiBold",
            "size": 20,
            "paragraphSpacingBefore": 14,
            "paragraphSpacing": 3
        },
        "title-mark4": {
            "color": "#9B82C9",
            "font": "AvenirNext-DemiBold",
           "paragraphSpacingBefore": 12,
            "paragraphSpacing": 3
        },

        "bold": {
            "color": "#996666",
            "font": ".AppleSystemUIFontHeavy"
        },
        
        "bold-left-mark": {
            "color": "#CC6666"
        },
        
        "bold-right-mark": {
            "color": "#CC6666"
        },
        
        "italic": {
            "color": "#774444"
        },
        
        "italic-left-mark": {
            "color": "#BB8888"
        },
        
        "italic-right-mark": {
            "color": "#BB8888"
        },
        
        "boldItalic": {
            "color": " #996666",
            "font": ".AppleSystemUIFontBoldItalic"
        },
        
        "boldItalic-left-mark": {
            "color": "#CC6666"
        },
        
        "boldItalic-right-mark": {
            "color": "#CC6666"
        },
        
        "todo": {
            "font": "noteplanstate",
            "color": "#CC6666",
            "paragraphSpacingBefore": 0,
            "paragraphSpacing": 2,
            "headIndent": 33
        },
        
        "checked-todo-characters": {
            "font": "noteplanstate",
            "headIndent": 33
        },
        
        "special-char": {
            "font": "Menlo-Regular"
        },
        
        "tabbed": {
            "headIndent": 43
        },
        
        "quote-mark": {
            "color": "#979893",
            "font": "noteplanstate",
            "headIndent": 33
        },
        
        "quote-content": {
            "color": "#434343",
            "type": "italic",
            "headIndent": 33
        },
        
        "markdown-link": {
            "color": "#E8A250"
        },
        
        "markdown-link-url": {
            "color": "#465E68"
        },
        
        "link": {
            "color": "#C88E4B"
        },
        
        "time": {
            "color": "#E36868"
        },
        
        "schedule-to-date-link": {
            "color": "#C88E4B"
        },
        
        "done-date": {
            "color": "#AAAAAA"
        },
        
        "schedule-from-date-link": {
            "color": "#C88E4B"
        },
        
        "note-title-link": {
            "color": "#97B853"
        },
        
        "hashtag": {
            "color": "#769EB3"
        },
        
        "attag": {
            "color": "#97B853"
        },
        
        "phonenumber": {
            "color": "#769EB3"
        },

        "attribute": {
            "regex": "\\v([\\w\\h]{3,})(::)",
            "color": "#D8A250",
            "matchPosition": 1
        },
        "attribute-right-colons": {
            "regex": "[\\v^]([^:]{3,})(::)",
            "color": "#B88230",
            "matchPosition": 2
        },

        "highlighted": {
            "regex": "(==)([^\\s].+)(==)",
            "backgroundColor": "#55D2D21B",
            "order": 35,
            "matchPosition": 2,
            "isRevealOnCursorRange": true
        },
        "highlighted-left-colon": {
            "regex": "(==)([^\\s].+)(==)",
            "color": "#AA45A2E5",
            "backgroundColor": "#7745A2E5",
            "isMarkdownCharacter": true,
            "isHiddenWithoutCursor": true,
            "isRevealOnCursorRange": true,
            "matchPosition": 1
        },
        "highlighted-right-colon": {
            "regex": "(==)([^\\s].+)(==)",
            "color": "#AA45A2E5",
            "backgroundColor": "#7745A2E5",
            "isMarkdownCharacter": true,
            "isHiddenWithoutCursor": true,
            "isRevealOnCursorRange": true,
            "matchPosition": 3
        },

        "NoteLinks-main": {
            "regex": "\\h(\\[\\[)(.*?)(\\]\\])",
            "matchPosition": 2,
            "color": "#C5487A"
        },
        "NoteLinks-left-brackets": {
            "regex": "\\h(\\[\\[)(.*?)(\\]\\])",
            "matchPosition": 1,
            "color": " #C5487A",
            "isMarkdownCharacter": true,
            "isRevealOnCursorRange": true
        },
        "NoteLinks-right-brackets": {
            "regex": "\\h(\\[\\[)(.*?)(\\]\\])",
            "matchPosition": 3,
            "color": " #C5487A",
            "isMarkdownCharacter": true,
            "isRevealOnCursorRange": true
        },

        "checked": {
            "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\] )(.*)",
            "color": "#BB97B863",
            "matchPosition": 0,
            "headIndent": 33
        },
        "checked-canceled": {
            "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] )(.*)",
            "matchPosition": 0,
            "color": "#44C74853",
            "headIndent": 33,
            "strikethroughStyle": 1
        },
        "checked-scheduled": {
            "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] )(.*)",
            "matchPosition": 0,
            "color": "#447B7C86",
            "headIndent": 33
        },

        "deadline": {
            "regex": "\\h+!(\\d{4}-\\d{2}-\\d{2})\\h*",
            "matchPosition": 1,
            "color": "#AA45A2E5"
        },

        "flagged-1": {
            "regex": "(^|\\v)\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)(\\!{1}\\h+.*)($|\\v)",
            "matchPosition": 4,
            "backgroundColor": "#FFE5E5"
        },

        "starred": {
            "regex": "(^|\\v)(.*⭐️.*)($|\\v)",
            "matchPosition": 2,
            "backgroundColor": "#FFE5E5"
        },

        "code": {
            "font": "CascadiaCode-Regular",
            "color": "#3B3AB2",
            "backgroundColor": "#DEEDFB"
        },
        "code-fence": {
            "theme": "github"
        },

        "eventID": {
            "regex": "(event:[A-F0-9-]{36})",
            "matchPosition": 1,
            "color": "#444444",
            "isHiddenWithoutCursor": true,
            "isRevealOnCursorRange": true
        }
    }
}
