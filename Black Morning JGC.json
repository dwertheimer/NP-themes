{
    "name": "Black Morning JGC",
    "style": "Dark",
    "author": {
        "name": "NotePlan",
        "email": "hello@noteplan.co"
    },

    "editor": {
        "backgroundColor": "#000000",
        "altBackgroundColor": "#1A1A1A",
        "tintColor": "#FF7C40",
        "tintColor2": "#FF7C40",
        "textColor": "#DFDFDF",
        "toolbarBackgroundColor": "#282828",
        "toolbarIconColor": "#DFDFDF",
        "menuItemColor": "#DFDFDF"
    },
    
    "styles": {
        
        "body": {
            "font": "AvenirNext-Regular",
            "size": 16,
            "color": "#DFDFDF",
            "lineSpacing": 2
        },
        
        "title": {
            "paragraphSpacingBefore": 18,
            "paragraphSpacing": 3
        },
    
        "title1": {
            "color": "#F0C000",
            "size": 28,
            "font": "AvenirNext-DemiBold"
        },
        
        "title2": {
            "color": "#F0C000",
            "size": 24,
            "paragraphSpacing": 2,
            "paragraphSpacingBefore": 20,
            "font": "AvenirNext-DemiBold"
        },
        
        "title3": {
            "color": "#F0C000",
            "size": 20,
            "paragraphSpacing": 2,
            "paragraphSpacingBefore": 10,
            "font": "AvenirNext-DemiBold"
        },
        
        "title4": {
            "color": "#F0C000",
            "size": 16,
            "paragraphSpacing": 2,
            "font": "AvenirNext-DemiBold"
        },

        "title-mark1": {
            "color": "#F0A000",
            "size": 28,
            "font": "AvenirNext-DemiBold"
        },
        
        "title-mark2": {
           "color": "#F0A000",
           "size": 24,
           "font": "AvenirNext-DemiBold"
        },
        
        "title-mark3": {
           "color": "#F0A000",
           "size": 20,
           "font": "AvenirNext-DemiBold"
        },

        "title-mark4": {
            "color": "#F0A000",
            "size": 16,
            "font": "AvenirNext-DemiBold"
        },

        "bold": {
            "font": "AvenirNext-Bold"
        },
        
        "bold-left-mark": {
            "color": "#6F6F6F"
        },
        
        "bold-right-mark": {
            "color": "#6F6F6F"
        },
        
        "italic": {
            "font": "AvenirNext-Italic"
        },
        
        "italic-left-mark": {
            "color": "#6F6F6F"
        },
        
        "italic-right-mark": {
            "color": "#6F6F6F"
        },
        
        "boldItalic": {
            "font": "AvenirNext-BoldItalic"
        },
        
        "boldItalic-left-mark": {
            "color": "#9F9F9F"
        },
        
        "boldItalic-right-mark": {
            "color": "#9F9F9F"
        },
        
        "code": {
            "font": "Menlo-Regular",
            "color": "#FFBC80"
        },
        
        "checked": {
            "color": "#BFBFBF",
            "headIndent": 33
        },
        
        "todo": {
            "font": "noteplanstate",
            "color": "#FF7C40",
            "headIndent": 33,
            "size": 16
        },
        
        "checked-todo-characters": {
            "font": "noteplanstate",
            "headIndent": 33,
            "size": 16
        },
        
        "special-char": {
            "font": "Menlo-Regular"
        },
        
        "tabbed": {
            "headIndent": 43
        },
        
        "quote-mark": {
            "color": "#80FF7C40",
            "font": "noteplanstate",
            "headIndent": 33,
            "size": 16
        },
        
        "quote-content": {
            "color": "#DFDFDF",
            "type": "italic",
            "headIndent": 33
        },
        
        "markdown-link": {
            "color": "#FF7C40"
        },
        
        "markdown-link-url": {
            "color": "#90DFDFDF"
        },
        
        "link": {
            "color": "#FF7C40"
        },
        
        "time": {
            "color": "#FF7C40"
        },
        
        "schedule-to-date-link": {
            "color": "#FF7C40"
        },
        
        "done-date": {
            "color": "#80FF7C40"
        },
        
        "schedule-from-date-link": {
            "color": "#FF7C40"
        },
        
        "note-title-link": {
            "color": "#FF7C40"
        },
        
        "hashtag": {
            "color": "#30AFAF"
        },
        
        "attag": {
            "color": "#50BFBF"
        },
        
        "phonenumber": {
            "color": "#FF7C40"
        },

        "flagged-1": {
            "regex": "(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)\\!{1}\\h+.*)($|\\v)",
            "matchPosition": 2,
            "backgroundColor": "#AAD0021B",
            "headIndent": 33
        }
    }
}
